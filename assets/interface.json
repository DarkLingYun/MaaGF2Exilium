{
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource/"
            ]
        },
        {
            "name": "B 服",
            "path": [
                "{PROJECT_DIR}/resource",
                "{PROJECT_DIR}/resource/resource_bilibili"
            ]
        },
        {
            "name": "美服",
            "path": [
                "{PROJECT_DIR}/resource",
                "{PROJECT_DIR}/resource/resource_en"
            ]
        }
    ],
    "name": "少女前线2：追放自动化助手",
    "custom_title": "匿影而生",
    "url": "https://github.com/DarkLingYun/MaaGF2Exilium",
    "mirrorchyan_rid": "MaaGF2Exilium",
    "task": [
        {
            "name": "社区每日操作",
            "entry": "CommunityDailyHelper",
            "check": false
        },
        {
            "name": "启动少女前线2",
            "entry": "StartGame",
            "check": true
        },
        {
            "name": "批量使用兑换码",
            "entry": "BatchRedeemCode",
            "check": false,
            "doc": "自动从远端仓库拉取兑换码（不保证兑换码全部可用）"
        },
        {
            "name": "日常任务前准备",
            "entry": "PrepareDailyTasks",
            "option": [
                "领取情报补给体力(限时)",
                "领取七日签到资源(限时)",
                "领取邮件",
                "通知调研邀请邮件",
                "使用完体力药水",
                "商城免费每日补给箱购买",
                "边界推进委托领取",
                "拆解武器配件",
                "拆解武器配件等级"
            ],
            "doc": "领取邮件需要主界面背景为人形",
            "check": true
        },
        {
            "name": "公共区日常",
            "entry": "PublicAreaDailyTasks",
            "option": [
                "访问休息室",
                "调度室派遣任务",
                "调度收益领取"
            ],
            "check": true
        },
        {
            "name": "[暂不可用]标准同调(金币本)",
            "entry": "AutoSweepBattle",
            "option": [
                "同调次数："
            ],
            "doc": "标准同调体力值每次消耗20"
        },
        {
            "name": "定向精研(刷配件)",
            "entry": "AutoSweepBattle",
            "option": [
                "配件类型：",
                "配件效果：",
                "配件刷取次数："
            ],
            "doc": "定向精研体力值每次消耗30"
        },
        {
            "name": "补给作战(人形培养)",
            "entry": "AutoSweepBattle",
            "option": [
                "补给资源：",
                "补给次数："
            ],
            "pipeline_override": {
                "enterSaridisjinTokenTask": {
                    "enabled": false
                }
            },
            "doc": "补给作战体力值存量条每次消耗20，其他均为10"
        },
        {
            "name": "心智勘测",
            "entry": "MindSurveyTask",
            "option": [
                "转录序体：",
                "勘测次数："
            ],
            "doc": "心智勘测体力值每次消耗30"
        },
        {
            "name": "模拟作战日常",
            "entry": "CombatSimulations",
            "option": [
                "首领挑战自律",
                "自动实兵演习",
                "演习次数",
                "自动调整战斗速度为 3 倍速",
                "实兵演习对象战力值范围",
                "自动领取峰值推定奖励",
                "自动兵棋推演",
                "推演次数"
            ],
            "check": true
        },
        {
            "name": "班组日常",
            "entry": "PlatoonTasks",
            "option": [
                "尘烟前线自动作战",
                "作战模式"
            ],
            "check": true
        },
        {
            "name": "版本活动日常",
            "entry": "VersionActivityTask",
            "option": [
                "物资模式活动选择",
                "物资商品兑换"
            ],
            "doc": "物资模式仅识别第五关"
        },
        {
            "name": "易物所商店兑换",
            "entry": "BulkExchange",
            "option": [
                "班组商店兑换",
                "调度商店兑换",
                "讯段交易兑换"
            ]
        },
        {
            "name": "领取奖励(日常，通行证和助战)",
            "entry": "ClaimRewardTasks",
            "check": true
        },
        {
            "name": "关闭游戏",
            "entry": "StopGame"
        }
    ],
